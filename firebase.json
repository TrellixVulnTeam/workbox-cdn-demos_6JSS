{
  "hosting": {
    "public": "public",
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "*.sh",
      "./*"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/", "function": "app"
      }, {
        "source": "/demo/*", "function": "app"
      }
    ]
  }
}
