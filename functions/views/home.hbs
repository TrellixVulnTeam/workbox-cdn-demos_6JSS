{{#each modules}}
  <section id="{{name}}">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 mx-auto">
        <h2>{{name}}</h2>
        <p>
          <a class="btn btn-outline-primary" href="/demo/{{name}}">Demo</a>
          <a class="btn btn-outline-primary disabled" href="#">Module Page</a>
          <a class="btn btn-outline-primary" href="/reference-docs/module-{{name}}" target="_blank">Reference Docs</a>
        </p>
      </div>
    </div>
  </div>
</section>
{{/each}}

